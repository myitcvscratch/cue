[access]
	inheritFrom = All-Projects
[submit]
[access "refs/*"]
	owner = group CUE Admins
[access "refs/for/refs/*"]
	label-Code-Review = -2..+2 group CUE Approvers
	label-Unity-Result = -2..+1 group CUE Approvers
	label-TryBot-Result = -2..+1 group CUE Approvers
	push = group CUE Approvers
	read = group CUE Approvers
	rebase = group CUE Approvers
	removeReviewer = group CUE Approvers
	submit = group CUE Approvers
[access "refs/heads/*"]
	create = group CUE Admins
	push = +force group CUE Admins
	pushMerge = group CUE Admins
	read = group CUE Admins
[access "refs/tags/*"]
	push = group CUE Admins
	push = +force group CUE Admins
[label "Unity-Result"]
	function = NoBlock
	value = -1 Fail
	value = 0 No score
	value = +1 Pass
	defaultValue = 0
[label "TryBot-Result"]
	function = NoBlock
	value = -1 Fail
	value = 0 No score
	value = +1 Pass
	defaultValue = 0
